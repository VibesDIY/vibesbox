{
  "name": "vibesbox",
  "version": "1.0.0",
  "description": "Ultra-minimal Cloudflare Worker for secure, isolated iframe hosting",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev --port 8989",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "check": "pnpm run format && wrangler deploy --dry-run",
    "format": "prettier --write ."
  },
  "keywords": [
    "cloudflare",
    "worker",
    "iframe",
    "vibesbox",
    "isolated"
  ],
  "author": "vibesbox",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.78.0"
  }
}
